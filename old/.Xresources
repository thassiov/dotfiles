urxvt.scrollBar:         false
urxvt.font:              xft:firamono:medium:size=9,xft:Monospace:size=9:antialias=true:hinting=true

! "Sweet Love" Scheme
! [https://terminal.sexy/#Hx8fwLGLSjY30XtJe4dIr4ZaU1xcd1dZbXFewLGLQC4urF0vZHA1j2hAREtLYURFWFxJl4ll]
! special
*.foreground:   #c0b18b
*.background:   #1f1f1f
*.cursorColor:  #c0b18b

! black
*.color0:       #4a3637
*.color8:       #402e2e

! red
*.color1:       #d17b49
*.color9:       #ac5d2f

! green
*.color2:       #7b8748
*.color10:      #647035

! yellow
*.color3:       #af865a
*.color11:      #8f6840

! blue
*.color4:       #535c5c
*.color12:      #444b4b

! magenta
*.color5:       #775759
*.color13:      #614445

! cyan
*.color6:       #6d715e
*.color14:      #585c49

! white
*.color7:       #c0b18b
*.color15:      #978965

urxvt*perl-lib: /home/user/urxvt-vim-scrollback

URxvt.perl-ext-common:  resize-font,fullscreen,vim-scrollback
URxvt.resize-font.smaller: C-S-Down
URxvt.resize-font.bigger: C-S-Up
URxvt.keysym.F11: perl:fullscreen:switch

! configure alt-s as the keybinding to enter vim scrollback mode.
urxvt.vim-scrollback: M-s

! configure alt-p as the keybinding to paste mode.
urxvt.vim-scrollback-paste: M-p
